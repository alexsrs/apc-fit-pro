# Configurações para Azure App Service
registry=https://registry.npmjs.org/
fund=false
audit=false
optional=true
save-exact=true
package-lock=true

# Otimizações para Azure e correção bug Rollup
production=false
legacy-peer-deps=true
maxsockets=20
fetch-retries=5
fetch-retry-mintimeout=10000
fetch-retry-maxtimeout=60000

# Correção para bug npm + rollup (@rollup/rollup-linux-x64-gnu)
# https://github.com/npm/cli/issues/4828
prefer-offline=false
cache-min=86400
fetch-timeout=300000
