# Configurações para Azure App Service
registry=https://registry.npmjs.org/
fund=false
audit=false
optional=false
save-exact=true
package-lock=true

# Otimizações para Azure
production=false
legacy-peer-deps=true
maxsockets=20
fetch-retries=5
fetch-retry-mintimeout=10000
fetch-retry-maxtimeout=60000
